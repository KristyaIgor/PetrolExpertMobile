            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:background="@android:color/transparent"
            android:scaleType="centerCrop"
            android:tint="#0B4A8F"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/icon_info" />

        <ImageView
            android:id="@+id/imageUserMain"
            android:layout_width="95dp"
            android:layout_height="95dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButtonInfo"
            app:srcCompat="@drawable/elipse_splash" />

        <TextView
            android:id="@+id/textCashWorkPlaceMain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="#B3000000"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonScanCardCorporativ"
            app:layout_constraintEnd_toEndOf="@+id/imageUserMain"
            app:layout_constraintStart_toStartOf="@+id/imageUserMain"
            app:layout_constraintTop_toBottomOf="@+id/textOperatorName"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintVertical_chainStyle="spread_inside" />


