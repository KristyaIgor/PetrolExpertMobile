        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/imageButtonLogout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/background_var3_buttons"
            android:clickable="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/buttonScanWithoutIdentify"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonScanMyDiscount">

            <ImageView
                android:id="@+id/imageView46464"
                android:layout_width="66dp"
                android:layout_height="45dp"
                android:layout_marginTop="32dp"
