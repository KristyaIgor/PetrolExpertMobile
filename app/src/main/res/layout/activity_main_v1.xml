            android:layout_height="0dp"
            android:background="@drawable/background_var3_buttons"
            android:clickable="true"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:elevation="15dp"
            android:outlineProvider="paddedBounds"
            android:translationZ="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toEndOf="@+id/imageButtonLogout"
            app:layout_constraintTop_toBottomOf="@+id/buttonScanCardCorporativ">

            <ImageView
                android:id="@+id/imageView4646"
                android:layout_width="66dp"
                android:layout_height="45dp"
                android:layout_marginTop="32dp"
                android:layout_marginBottom="24dp"
                app:layout_constraintBottom_toTopOf="@+id/textView224"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="spread"
                app:srcCompat="@drawable/other_pay_new" />

            <TextView
                android:id="@+id/textView224"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/alta_achitare"
                android:textColor="@color/white"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView4646" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/buttonScanCardCorporativ"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/background_var2_buttons"
            android:clickable="true"
            app:layout_constraintBottom_toTopOf="@+id/buttonScanWithoutIdentify"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toEndOf="@+id/buttonScanMyDiscount"
            app:layout_constraintTop_toBottomOf="@+id/textCashWorkPlaceMain">

            <ImageView
                android:id="@+id/imageView33546"
                android:layout_width="66dp"
                android:layout_height="45dp"
                android:layout_marginTop="32dp"
                android:layout_marginBottom="24dp"
                app:layout_constraintBottom_toTopOf="@+id/textView22464"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="spread"
                app:srcCompat="@drawable/card_corporativ_new" />

            <TextView
                android:id="@+id/textView22464"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_medium"
                android:text="@string/achitare_ncard_corporativ"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView33546" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/buttonScanMyDiscount"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/background_var1_buttons"
            android:clickable="true"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:elevation="10dp"
            android:outlineProvider="paddedBounds"
            android:translationZ="2dp"
            app:layout_constraintBottom_toTopOf="@+id/imageButtonLogout"
            app:layout_constraintEnd_toStartOf="@+id/buttonScanCardCorporativ"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/buttonScanCardCorporativ">

            <ImageView
                android:id="@+id/imageView3354646"
                android:layout_width="66dp"
                android:layout_height="45dp"
                android:layout_marginTop="32dp"
                android:layout_marginBottom="24dp"
                app:layout_constraintBottom_toTopOf="@+id/textView2245464"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.76"
                app:layout_constraintVertical_chainStyle="spread"
